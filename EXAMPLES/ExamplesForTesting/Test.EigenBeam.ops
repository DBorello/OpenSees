# Elastic properties
set E 1.0
set A 2.0
set I 4.0

# Beam length and hinge length ratio
set L    2.0
set beta 0.2

# Nodal coordinates
set x1 0.0
set x2 $L

# Define model builder
model BasicBuilder -ndm 2 -ndf 3

# Nodes
node 1 $x1 0.0
node 2 $x2 0.0

# Beam geometric transformation
geomTransf Linear 1

# Define element
#                         tag ndI ndJ  A  E  I transfTag
element elasticBeamColumn  1   1   2  $A $E $I     1

# Compute element eigenvalues
set eigenValues [eigen standard 6]

puts ""
puts "Computed element eigenvalues"
puts "----------------------------"
for {set mode 0} {$mode < 6} {incr mode} {
    puts "\tMode [expr $mode+1]: [lindex $eigenValues $mode]"
}

puts ""
puts "Exact non-zero eigenvalues"
puts "--------------------------"
puts "\t2EA/L:            [expr 2*$E*$A/$L]"
puts "\t2EI/L:            [expr 2*$E*$I/$L]"
puts "\t6EI/L + 24EI/L^3: [expr 6*$E*$I/$L + 2*12*$E*$I/($L*$L*$L)]"
